@{
    Layout = null;
}
@model LoginViewModel
<!DOCTYPE html>
<html class="no-js" lang="zxx">

<!-- Mirrored from demo.graygrids.com/themes/blink/signin.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 01 Jun 2022 09:48:26 GMT -->
<head>
    <meta charset="utf-8" />
    <meta http-equiv="x-ua-compatible" content="ie=edge" />
    <title>Sign In - Blink Startup & SaaS Bootstrap 5 Template.</title>
    <meta name="description" content="" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="shortcut icon" type="image/x-icon" href="~/assets/images/favicon.svg" />

    <link rel="stylesheet" href="~/assets/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/assets/css/LineIcons.3.0.css" />
    <link rel="stylesheet" href="~/assets/css/animate.css" />
    <link rel="stylesheet" href="~/assets/css/tiny-slider.css" />
    <link rel="stylesheet" href="~/assets/css/glightbox.min.css" />
    <link rel="stylesheet" href="~/assets/css/main.css" />
</head>
<body>
  

    <div class="account-login section">
        <div class="container">
            <div class="row">
                <div class="col-lg-6 offset-lg-3 col-md-10 offset-md-1 col-12">
                    <form action="/Acount/Lockin" class="card login-form inner-content" method="post">
                        <div class="card-body">
                            <div class="title">
                                <h3>Sign In Now</h3>
                                <p>Sign in by entering the information below.</p>
                            </div>
                            <div class="input-head">
                                <div class="form-group input-group">
                                    <label><i class="lni lni-envelope"></i></label>
                                    <input asp-for="Email" class="form-control" type="email" id="reg-email" placeholder="Enter your email" required>
                                    <span asp-validation-for="Email" class="text-danger"></span>
                                </div>
                                <div class="form-group input-group">
                                    <label><i class="lni lni-lock-alt"></i></label>
                                    <input asp-for="Password" class="form-control" type="password" id="reg-pass" placeholder="Enter your password" required>
                                    <span asp-validation-for="Password" class="text-danger"></span>
                                </div>
                            </div>
                            <div class="d-flex flex-wrap justify-content-between bottom-content">
                                <div class="form-check">
                                    <input asp-for="StayLogedIn" type="checkbox" class="form-check-input width-auto" id="exampleCheck1">
                                    <label class="form-check-label">Remember me</label>
                                </div>
                                <div asp-validation-summary="ModelOnly"></div>
                                <a class="lost-pass" href="reset-password.html">Forgot password?</a>
                            </div>
                            <div class="button">
                                <button class="btn" type="submit">Sign In</button>
                                <a class="btn alt" href="signup.html">Create Account</a>
                            </div>
                            <ul class="alt-option">
                                <li><span>Or Sign In with</span></li>
                                <li><a href="javascript:void(0)">Facebook</a></li>
                                <li><a href="javascript:void(0)">Twitter</a></li>
                                <li><a href="javascript:void(0)">Google</a></li>
                            </ul>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>



    <a href="#" class="scroll-top">
        <i class="lni lni-chevron-up"></i>
    </a>

    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/wow.min.js"></script>
    <script src="~/assets/js/tiny-slider.js"></script>
    <script src="~/assets/js/glightbox.min.js"></script>
    <script src="~/assets/js/count-up.min.js"></script>
    <script src="~/assets/js/main.js"></script>
</body>

<!-- Mirrored from demo.graygrids.com/themes/blink/signin.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 01 Jun 2022 09:48:26 GMT -->
</html>